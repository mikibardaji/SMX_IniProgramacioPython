# for del 1 al 19
# for x in range(1, 19):
#  print(x)
# for del 1 al 19 augmentant de 2 amb 2
for x in range(1, 19, 2):
  print(x)
# for desde el 10 fins al 100, augmentant de 5 amb 5
# i la variable s'anomeni num
for num in range(10, 101, 5): #pongo 101 para que muestre el 100
  print(num)
# Bucle que vaya del 150 al 100(mostri el 100), 
# y muestre solo de 10 en 10...
# valor variable bucle 
for num in range(150, 99, -10): #pongo 101 para que muestre el 100
  print(num)
# para romper el bucle es break
# ira de 150 a 99 pero cuando num sea mas peque√±o que
# 130 , le haremos parar
for num in range(150, 99, -10): #pongo 101 para que muestre el 100
  print(num)
  if (num<130):
    print("Es menor de 130")
    break
